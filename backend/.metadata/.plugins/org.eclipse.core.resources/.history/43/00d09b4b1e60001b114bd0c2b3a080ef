package com.nl.fos.service;
import java.util.List;

import com.nl.fos.model.FoodCategory;

public interface CategoryService {

    List<FoodCategory> findAll();

    FoodCategory findByCategoryType(Integer categoryType);

    List<FoodCategory> findByCategoryTypeIn(List<Integer> categoryTypeList);

    FoodCategory save(FoodCategory productCategory);


}
